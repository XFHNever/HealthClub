<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
     "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
      "http://struts.apache.org/dtds/struts-2.0.dtd">
 <struts>
      <package name="struts2" namespace="/"
          extends="struts-default">
          <action name="login" class="LoginAction">
              <result name="member">/member/memhome.jsp</result>
              <result name="waiter">/member/Success.jsp</result>
              <result name="manager">/member/Success.jsp</result>
              <result name="fail">/member/login_fail.jsp</result>
          </action>
          <action name="register" class="RegisterAction">
          	  <result name="success">/member/register_success.jsp</result>
          	  <result name="fail">/member/register_fail.jsp</result>
          </action>
      </package>
  </struts>
